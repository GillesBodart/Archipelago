MatchNeo4J(clazz,props) ::= <<
    MATCH (n:<clazz.simpleName>)
    WHERE
    <props : createWhere(); separator=",\n">
    RETURN
    <props : createReturn()>
    ID(n) AS InternalId
>>

createWhere(property) ::= "\tn.<property> = {<property>\}"
createReturn(property) ::= "\tn.<property> AS <property>,"



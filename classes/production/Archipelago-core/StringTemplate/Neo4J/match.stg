MatchNeo4J(clazz,props) ::= <<
    MATCH (n:<clazz.simpleName>)
    WHERE
    <props : createWhere(); separator=" AND ">
    RETURN
    <props : createReturn()>
    ID(n) AS InternalId
>>

createWhere(property) ::= "n.<property> = {<property>\}"
createReturn(property) ::= "n.<property> AS <property>,\n"
